<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="850.0" prefWidth="850.0" stylesheets="stylesheets/DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.40" 
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="hh.fernuni.rentamovie.rent.adapter.RentOverviewController">
   <children>
         <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="750.0" prefWidth="850.0">
              <children>
                 <TableView fx:id="rentTable" layoutX="-12.0" layoutY="49.0" prefHeight="500.0" prefWidth="780.0" 
                 	AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                   <columns>
                     <TableColumn fx:id="customerColumn" prefWidth="100.0" text="Customer" />
                     <TableColumn fx:id="copyColumn" prefWidth="100.0" text="Copy" />
                     <TableColumn fx:id="startDateColumn" prefWidth="100.0" text="Start Date" />
                     <TableColumn fx:id="endDateColumn" prefWidth="100.0" text="End Date" />
                   </columns>
                    <columnResizePolicy>
                       <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                 </TableView>
                 <ButtonBar layoutX="0.0" layoutY="1000.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
                    <buttons>
                      <Button fx:id="newButton" mnemonicParsing="false" onAction="#handleNewRent" text="New..." />
                    </buttons>
                  </ButtonBar>
              </children>
          </AnchorPane>
   </children>
</AnchorPane>
