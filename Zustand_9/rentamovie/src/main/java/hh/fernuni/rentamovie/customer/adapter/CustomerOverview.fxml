                      <Button fx:id="newButton" mnemonicParsing="false" onAction="#handleNewCustomer" text="New..." />
                      <Button fx:id="saveButton" mnemonicParsing="false" onAction="#handleSaveCustomer" text="Save..." />
                    </buttons>
                  </ButtonBar>
               </children>
            </AnchorPane>
        </items>
