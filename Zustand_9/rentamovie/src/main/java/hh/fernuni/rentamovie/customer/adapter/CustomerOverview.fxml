<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="563.0" prefWidth="793.0" stylesheets="stylesheets/DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.40" 
